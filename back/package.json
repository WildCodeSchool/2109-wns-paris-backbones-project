{
	"name": "backbones-back",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"type": "commonjs",
	"scripts": {
		"build": "tsc",
		"start": "cd build && NODE_ENV=prod node index.js",
		"dev": "cd src && NODE_ENV=prod ts-node index.ts",
		"seed:test": "cd src && export NODE_ENV=test && ts-node seedDatabase.ts",
		"seed:dev": "cd src && export NODE_ENV=prod && ts-node seedDatabase.ts",
		"seed:prod": "cd build && export NODE_ENV=prod && node seedDatabase.js",
		"test": "yarn seed:test ; jest --detectOpenHandles",
		"test:coverage": "yarn seed:test ; jest --coverage --detectOpenHandles",
		"lint": "eslint . "
	},
	"devDependencies": {
		"@types/node": "^8.0.29",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"eslint": "^8.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.4.1",
		"ts-node-dev": "^1.1.8"
	},
	"dependencies": {
		"@types/better-sqlite3": "^7.4.1",
		"@types/jest": "^27.0.3",
		"apollo-server": "^3.4.0",
		"better-sqlite3": "^7.4.5",
		"class-validator": "^0.13.1",
		"dotenv": "^10.0.0",
		"graphql": "^15.7.1",
		"jest": "^27.3.1",
		"pg": "^8.4.0",
		"reflect-metadata": "^0.1.10",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.4.0",
		"type-graphql": "^1.1.1",
		"typeorm": "0.2.38",
		"typescript": "^4.4.4"
	}
}
